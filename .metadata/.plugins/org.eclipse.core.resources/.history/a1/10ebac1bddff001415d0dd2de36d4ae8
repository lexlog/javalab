package third_test;

import java.util.Random;

public class Car implements Runnable {
	int money=(new Random()).nextInt(1500)+30;
	final int id; 
	int pumpNumber=(new Random()).nextInt(3)+1; 
	Admin adm;
	
	public Car(Admin a,int n) {
		adm=a;
		id=n;
		System.out.println("Client"+id+": Hi,I'm from Pump â„–"+pumpNumber+", have "+money+" money points");
	}

	@Override
	public void run() {
		adm.payment(money, id, pumpNumber);	
		
	}

}
